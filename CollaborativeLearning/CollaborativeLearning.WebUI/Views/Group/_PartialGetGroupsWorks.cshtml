@model ICollection<CollaborativeLearning.Entities.GroupWork>
<table class="table table-hover table-responsive">
    <tbody class="table-striped">
        
        @if (Model.Count != 0)
        {
            foreach (var item in Model)
            {
            <tr class="success">
                <td>
                    @item.Work.Name
                </td>
                <td>
                    @item.SubmissionDate
                </td>
            </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="4" width="100%">
                    Any unseen submited work is not found!
                </td>
            </tr>
        }

    </tbody>
</table>

