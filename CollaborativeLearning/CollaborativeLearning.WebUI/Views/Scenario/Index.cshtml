@{
    ViewBag.Title = "Detail";
}

<ul class="nav nav-tabs">
  <li class="active"><a href="#task" data-toggle="tab">Student Tasks</a></li>
  <li><a href="#action" data-toggle="tab">Action Plan</a></li>
  <li><a href="#resources" data-toggle="tab">Resources</a></li>
  <li><a href="#works" data-toggle="tab">Submited Works</a></li>
  @*<li><a href="#feedback" data-toggle="tab">Mento Feedbacks</a></li>*@
</ul>

<!-- Tab panes -->
<br />
<div class="tab-content">
  <div class="tab-pane active" id="task">
        
        <div id="myPartialViewDiv" class="row" style="margin-left:-13px;">
            <div class="col-lg-12">
                <div id="studentTaskContainer">
                    @Html.Action("_PartialStudentTask", "Task", new {scenarioId = ViewBag.scenarioId})
                </div>
            </div>
        </div>
  </div>




  <div class="tab-pane clearfix" id="action">
       
      @Html.Action("_PartialActionPlan", "ActionPlan", new {scenarioId = ViewBag.scenarioId})
  </div>


  <div class="tab-pane" id="resources">

      @Html.Action("_PartialResources", "Scenario", new { id = ViewBag.scenarioId })

  </div>
  <div class="tab-pane" id="works">
      
      @Html.Action("_PartialWork", "Work", new {scenarioId = ViewBag.scenarioId})
  </div>
  @*<div class="tab-pane" id="feedback">
      @Html.Action("_PartialFeedbacks", "Scenario")

  </div>*@
</div>

<script>
    function searchAdmin() {
        var URL = "Task/_PartialStudentTask";

        $.get(URL, function (data) {
            $("#myPartialViewDiv").html(data);
        })
    }
</script>