@model  ICollection<CollaborativeLearning.Entities.Scenario>

@{
    if (Model != null)
    {

        if (Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <li><a tabindex="-1" href="@Url.Action("Detail", "Scenario", new { id = item.Id, semesterId = ViewBag.SemesterID })">@item.Name</a></li>
            }
        }
        else
        {
            <li> No Scenario</li>

        }
    }
}