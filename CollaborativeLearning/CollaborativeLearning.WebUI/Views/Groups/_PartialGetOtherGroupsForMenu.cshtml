@model  ICollection<CollaborativeLearning.Entities.Group>

@{
    if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.GroupName, "Show", "Groups", new { GroupId = item.Id }, new { tabindex = "-1" })</li>
        }
    }else{
        <li> No other group</li>
    }

}